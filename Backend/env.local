# Configurazione AI Models
# =========================

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash  # Opzioni: gemini-1.5-flash, gemini-1.5-pro

# OpenAI Configuration  
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo  # Opzioni: gpt-4o, gpt-4o-mini, gpt-3.5-turbo

# Configurazione AI Provider
AI_PROVIDER=auto  # Opzioni: gemini, openai, auto, local (usa solo analisi locale)

# Configurazione ambiente
ENVIRONMENT=development
DEBUG=true 

# Browserbase Configuration (opzionale per Stagehand)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here
